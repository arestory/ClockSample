<?xml version="1.0" encoding="utf-8"?>
<resources>



    <declare-styleable name="LoadingView">
        <attr name="circleColor" format="color"></attr>
        <attr name="radius" format="integer"></attr>
    </declare-styleable>



    <declare-styleable name="WatchView">
        <attr name="sColor" format="color"></attr>
        <attr name="mColor" format="color"></attr>
        <attr name="hColor" format="color"></attr>
